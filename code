// Online C compiler to run C program online
//ADDITON OF TWO MATRIX
#include <stdio.h>

void main() {
    int mat1[2][3],mat2[2][3],result[2][3];
    char i,j;
    for(i=0;i<2;i++){
        for(j=0;j<3;j++){
            scanf("Enter the mat 1:%d",&mat1[i][j]);
        }
    }
     for(i=0;i<2;i++){
        for(j=0;j<3;j++){
            scanf("Enter the mat 2:%d",&mat2[i][j]);
        }
    }
    for(i=0;i<2;i++){
        for(j=0;j<3;j++){
            result[i][j]= mat1[i][j]+mat2[i][j];
            printf("%d  ",result[i][j]);
        }
        printf("\n");
    }
    /* for(i=0;i<2;i++){
        for(j=0;j<3;j++){
            printf("%d",result[i][j]);
        }
       printf("\n");
    }*/

}


//Find the element in the 2d array:
// Online C compiler to run C program online
#include <stdio.h>

int main() {
    char math1[][10]={"rath","inam","tech"};
    int i,j;
    for(i=0;i<3;i++){
        for(j=0;j<4;j++){
            //scanf("Enter the mat 1:%d",&mat1[i][j]);
            if(math1[i][j]=='n'){
                printf("%d %d",i,j);
            }
        }
    }
   
    return 0;  
    }
   
