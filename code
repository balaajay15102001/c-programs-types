// Online C compiler to run C program online
#include <stdio.h>

void main() {
    int mat1[3][4],mat2[3][4],math3[3][4],result[3][4];
    char i,j;
    for(i=0;i<2;i++){
        for(j=0;j<3;j++){
            scanf("Enter the mat 1:%d",&mat1[i][j]);
        }
    }
     for(i=0;i<2;i++){
        for(j=0;j<3;j++){
            scanf("Enter the mat 2:%d",&mat2[i][j]);
        }
    }
     for(i=0;i<2;i++){
        for(j=0;j<3;j++){
            scanf(" Enter the mat 3: %d",&mat3[i][j]);
        }
    }
    for(i=0;i<2;i++){
        for(j=0;j<3;j++){
            result[i][j]= mat1[i][j]+mat2[i][j];
            printf("%d  ",result[i][j]);
        }
        printf("\n");
    }
    /* for(i=0;i<2;i++){
